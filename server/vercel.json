{
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "https://school-inventory-management-system-server.vercel.app/api/:path*"
      }
    ],
    "headers": [
      {
        "source": "/api/:path*",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "https://school-inventory-management-system.vercel.app"
          },
          {
            "key": "Access-Control-Allow-Methods",
            "value": "GET, POST, PUT, DELETE, OPTIONS"
          },
          {
            "key": "Access-Control-Allow-Headers",
            "value": "X-Requested-With, Content-Type, Authorization"
          }
        ]
      }
    ]
  }
  